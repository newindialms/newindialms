<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/textColor"
    tools:context="edu.thapar.newindialms.StudentPicProgram">

    <include
        android:id="@+id/student_enroll_toolbar"
        layout="@layout/student_enroll_toolbar"></include>

    <TextView
        android:id="@+id/studentschedule_display_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/student_enroll_toolbar"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="10dp"
        android:text=""
        android:textAlignment="center"
        android:textColor="@color/studentcolorPrimaryDark"
        android:textSize="24sp" />

    <android.support.v7.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/studentcheduledisplaylist_ListView"
        android:layout_below="@+id/studentschedule_display_title" />
</RelativeLayout>
