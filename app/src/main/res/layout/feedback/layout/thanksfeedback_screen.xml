<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/textColor"
    tools:context="edu.thapar.newindialms.SubmitFeedBackScreen">

    <include
        android:id="@+id/student_enroll_toolbar"
        layout="@layout/student_enroll_toolbar"/>

    <TextView
        android:id="@+id/Thankyoutext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="231dp"
        android:padding="10dp"
        android:text="@string/thanks_feedback"
        android:textColor="@color/studentcolorPrimary"
        android:textSize="20sp" />


    <Button
        android:id="@+id/thankyoubutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/studentcolorPrimaryDark"
        android:layout_below="@id/Thankyoutext"
        android:layout_centerHorizontal="true"
        android:textAlignment="center"
        android:textColor="@color/textColor"
        android:textSize="14sp"
        android:text="@string/close"/>
</RelativeLayout>