<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/textColor"
    tools:context="edu.thapar.newindialms.MyFeedbackEnrolledCourse">

    <include
        android:id="@+id/student_enroll_toolbar"
        layout="@layout/student_enroll_toolbar"/>

    <TextView
        android:id="@+id/myfeedbackenrolledcourses_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/student_enroll_toolbar"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="26dp"
        android:text="@string/enrolledcourses_title"
        android:textAlignment="center"
        android:textColor="@color/studentcolorPrimaryDark"
        android:textSize="24sp" />

    <RelativeLayout
        android:id="@+id/relative1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/myfeedbackenrolledcourses_title"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="23dp"
        android:background="@drawable/student_rounded_button">

        <TextView
            android:id="@+id/myfeedbackenrolledcourselist_name"
            android:layout_width="fill_parent"
            android:layout_height="60dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:width="0dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingStart="10dp"
            android:paddingTop="10dp"
            android:text="@string/enrolledcourses_title"
            android:textColor="@color/studentcolorPrimaryDark"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/myfeedback_rightarrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"
            android:layout_marginTop="20dp"
            android:background="@color/textColor"
            android:contentDescription="@string/next"
            app:srcCompat="@drawable/student_right_arrow" />
    </RelativeLayout>

</RelativeLayout>
